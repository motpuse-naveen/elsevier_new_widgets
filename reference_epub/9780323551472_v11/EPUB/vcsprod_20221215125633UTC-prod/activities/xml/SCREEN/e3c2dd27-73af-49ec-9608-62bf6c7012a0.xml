  <block refid="d2391d61-89a4-4717-bf9d-c32882321331">
    <FIBDD>
  <form>
    <questions>
      <question>
        <stimulus>
          <imagePart>
            <image/>
            <imageControls>
              <imageScale value="full-screen"/>
              <imageAlignment value="center"/>
